//to print age 

#include <stdio.h>

int main(){
    int age = 30;
    
    printf("age is %d",age);
    
    return 0;
} 



// to print character 

#include <stdio.h>

int main(){
    
    char hashtag ='#';
    printf("hashtag is %c \n",hashtag);
    return 0;
}    



// to take an input

#include <stdio.h>

int main(){
   int age;
   printf("enter age");
   scanf("%d",&age);
   printf("age is : %d",age);
   return 0;
}   


// to output of sum a and b

#include <stdio.h>

int main(){
   int a,b;
   printf("enter a");
   scanf("%d",&a);
   
   
   printf("enter b");
   scanf("%d",&b);
   
   int sum= a+b;
   printf("sum is : %d",a+b);
   return 0;
}


// area of square

#include <stdio.h>

int main(){
    int side;
    printf("enter side");
    scanf("%d",&side);
    printf("area is :%d", side*side);
   return 0;
}
