//to print age 

#include <stdio.h>

int main(){
    int age = 30;
    
    printf("age is %d",age);
    
    return 0;
} 



// to print character 

#include <stdio.h>

int main(){
    
    char hashtag ='#';
    printf("hashtag is %c \n",hashtag);
    return 0;
}    



// to take an input

#include <stdio.h>

int main(){
   int age;
   printf("enter age");
   scanf("%d",&age);
   printf("age is : %d",age);
   return 0;
}   
